<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >
    
    <item >
    	<shape 
    		android:shape="rectangle">
    	<solid android:color="#FFFFFF" />
		<stroke android:width="1dp" android:color="@color/dark_grey"/>
    	<padding 
        	android:top="1dp"
        	android:bottom="1dp" />
    	<solid 
        	android:color="@color/white"
        	/>
    	<corners 
        	android:radius="0dp"/>
		</shape>
    </item>
    
</layer-list>
